<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "https://checkstyle.org/dtds/suppressions_1_1.dtd">
<suppressions>
    <suppress files=".java" checks="Header"/>
    <suppress files=".java" checks="JavadocType" />
    <suppress files=".java" checks="JavadocMethod"/>
    <suppress files=".java" checks="JavadocField"/>
    <suppress files=".java" checks="JavadocComment"/>
    <suppress  checks="JavadocPackage" files=".*"/>
    <suppress checks="NeedThisCheck" files=".*" />
</suppressions>
